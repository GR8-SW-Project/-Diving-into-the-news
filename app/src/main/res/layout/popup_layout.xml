        tools:layout_editor_absoluteY="186dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="62dp"
        android:layout_height="48dp"
        android:text="취소"
        tools:layout_editor_absoluteX="272dp"
        tools:layout_editor_absoluteY="186dp" />

</RelativeLayout>